{
  "name": "matchkeys",
  "description": "Return the resolved filepaths to named npm module dependencies based on keywords in package.json. Minimatch patterns can be used, and Lo-Dash templates can be used to load filepaths into Grunt config.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/matchkeys",
  "author": {
    "name": "Jon Schlinkert",
    "url": "https://github.com/jonschlinkert"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/matchkeys.git"
  },
  "bugs": {
    "url": "https://github.com/jonschlinkert/matchkeys/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jonschlinkert/matchkeys/blob/master/LICENSE-MIT"
    }
  ],
  "main": "matchkeys.js",
  "engines": {
    "node": ">= 0.8.0"
  },
  "dependencies": {
    "lodash": "~1.3.1",
    "chalk": "~0.2.1",
    "resolve-dep": "~0.1.2"
  },
  "devDependencies": {
    "node-foo": "*"
  },
  "keywords": [
    "foo",
    "bar",
    "files",
    "lodash",
    "glob",
    "chalk",
    "grunt config",
    "load-grunt-tasks",
    "grunt",
    "matchdep",
    "lo-dash templates",
    "load",
    "lodash templates",
    "match",
    "minimatch",
    "pattern",
    "require resolve",
    "require",
    "resolve file paths",
    "resolve node module",
    "resolve npm module",
    "resolve paths",
    "resolved filepath"
  ]
}
